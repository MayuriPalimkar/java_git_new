package string;

public class string10 {
 
	public static void main(String[] args) {
		//new Reverse_String().Reverse();
		
	
	public void Reverse() {
		String str = "Welcome to java and automation";
		
		String str1[]= new String[30];
		int count = 0;
		str1[1]= "";
		str1[2]= "";
		str1[3]= "";
		str1[4]= "";
		str1[5]= "";
		str1[6]= "";

		for (int i =0; i<str.length();i++) {
			if(str.charAt(i)==' ') {
				count++;
				
			}
			else {
				str1[count]=str1[count]+str.charAt(i);
			}
		}
		System.out.println(str1[6]+" "+str1[5]+" "+str1[4]+" "+str1[3]+" "+str1[2]+" "+str1[1]+" ");
	}
}
}