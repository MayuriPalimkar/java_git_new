package array;

public class ARRAY9 {

	public static void main (String [] args) {
		int arr[] = new int[4];
		
		arr[0] = 1;
		arr[1] = 2;
		arr[2] = 3;
		arr[3] = 4;
		
		System.out.println(arr.length);
		System.out.println();
		for (int i=0; i< arr.length; i++) {
			System.out.println(arr[i]);
			
		}
	}
}
